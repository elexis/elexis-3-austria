  <xsd:complexType name="tpcAdresse">
    <xsd:all>
      <xsd:element ref="AdresseKey" minOccurs="0"/>
      <xsd:element name="AdresseArt" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Art der Adresse laut Codeliste E01 (ÖNorm), wie z.B. Wohnadresse, Verständigungsadresse</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AdresseStruktur" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Struktur für Strasse und Hausnummer laut Codeliste E03 (ÖNorm)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PostLeitzahl" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Postleitzahl</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Ort" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Ort</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Strasse" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Strasse entweder mit Hausnummer oder ohne ( dann steht die Hausnummer in einer eigenen Komponente).Die Art der Darstellung von Strasse und Hausnummer wird über die Codeliste E03 in der Komponente Adressestruktur bekannt gegeben</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="HausNummer" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Hausnummer der Strasse, wenn diese nicht in der Strasse enthalten ist (in Abhängigkeit vom Code in der Komponente Adressestruktur)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Staat" type="tpsNationalityTyp" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Staat der Adresse laut Codeliste D28 (ISO-Codeliste, ÖNorm)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="GesamteAdresse" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>die gesamte Adresse wird  in der Form der unstrukturierten Adresse dargestellt und ist im Zusammenhang mit der Komponente Adressestruktur, Code 5 aus der Codeliste E03 (ÖNorm), angegeben</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="tpcAufenthalt">
    <xsd:all>
      <xsd:element ref="AufenthaltsKey"/>
      <xsd:element ref="BetriebsStelleKey" minOccurs="0"/>
      <xsd:element ref="BetriebsStelleText" minOccurs="0"/>
      <xsd:element name="Patient" type="tpcPerson" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Identifikationen und Informationen zu einem Patienten</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="Beziehungen" minOccurs="0"/>
      <xsd:element ref="FamilienName" minOccurs="0"/>
      <xsd:element ref="VorName" minOccurs="0"/>
      <xsd:element ref="Geschlecht" minOccurs="0"/>
      <xsd:element ref="GeburtsDatum" minOccurs="0"/>
      <xsd:element ref="SVNummer" minOccurs="0"/>
      <xsd:element name="AufenthaltsArt" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Informationen zu der Art des Aufenthalts (Art der Behandlung in einer medizinischen Organisation, wie stationär, Tagesklinik, ambulant) einer Person</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:string">
              <xsd:attributeGroup ref="agrCodeList"/>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Status" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Allgemeine Statusbeschreibungen</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="aufgenommen"/>
            <xsd:enumeration value="entlassen"/>
            <xsd:enumeration value="storniert"/>
            <xsd:enumeration value="voradministriert"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="AufnahmeZeit" type="tpsDatumZeitEmpty" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Datum und Uhrzeit des Beginns des Aufenthalts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AufnahmeArt" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Informationen zu der Art der Aufnahme (zB. Einweisung, Rettung, selbst) in einer medizinischen Organisation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:string">
              <xsd:attributeGroup ref="agrCodeList"/>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="EntlassungsZeit" type="tpsDatumZeitEmpty" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>wk.makro: RT_ENTLASSUNGS_ZEIT</xsd:documentation>
          <xsd:documentation>Datum und Uhrzeit des Endes des Aufenthalts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EntlassungsArt" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Informationen zu der Art der Entlassunge (zB. Entlassung aus dem Krankenhaus, Verlegung, Urlaub, Sterbefall ) aus einer medizinischen Organisation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:string">
              <xsd:attributeGroup ref="agrCodeList"/>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="AufenthaltsPfad" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Wenn grosse Organisationen mit File-gestuetzten Systemen arbeiten ist es sinnvoll den Pfad zu den Aufenthaltsdaten zu teilen. Wenn dies geschieht, kann es hier angegeben werden. Trennung der Pfade durch \.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="tpcDokument">
    <xsd:all>
      <xsd:element ref="DokumentenKey"/>
      <xsd:element ref="AufenthaltsKey"/>
      <xsd:element ref="BetriebsStelleKey"/>
      <xsd:element ref="DVRNummer" minOccurs="0"/>
      <xsd:element name="DokumentenBesitzer" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Wird das Original-Dokument nicht beim Ersteller aufbewahrt, so ist hier die Stelle der Aufbewahrung anzugeben.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AenderungsZeit" type="tpsDatumZeit" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Falls nach der Erstellung des Dokumentes Aenderungen durchgefuehrt wurden, ist hier die Zeit der letzten Aenderung anzugeben. Nach einer Versendung eines Dokumentes sind keine Aenderungen mehr erlaubt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DokumentenUeberschrift" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Überschrift des Dokumentes. Sollte so gewählt sein, dass das Original beim Ersteller gefunden werden kann.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="DokumentenZeit"/>
      <xsd:element name="DokumentenStatus" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Codeliste: In welchem organisatorischen Status befindet sich dieses Dokument, zB. ist es ein Vorbefund, wurde es bereits vidiert etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DokumentenStatusText" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Text zu Dokumentenstatus</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Qualifier" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Codeliste: Die Art des Dokumentes (z.B. Laborbefund, Arztbrief, etc.)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="QualifierText" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Text zu Qualifier</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AnfordererKey" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Falls das Dokument das Ergebnis einer Anforderung von außen präsentiert, ist hier der BetriebsStellenKey des Anforderers anzugeben.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AnfordererText" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Gesamter Name des Anforderers.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DokumentenPfad" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Technischer Parameter: Ist anzugeben falls Pfade und zugehörige Keys unterschiedlich aufgebaut sind.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SperrKey" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Ist reserviert für Zugriffssperren.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="tpcPerson">
    <xsd:all>
      <xsd:element ref="FamilienName"/>
      <xsd:element ref="VorName"/>
      <xsd:element ref="Titel" minOccurs="0"/>
      <xsd:element ref="GeburtsDatum" minOccurs="0"/>
      <xsd:element ref="Geschlecht" minOccurs="0"/>
      <xsd:element ref="eMail" minOccurs="0"/>
      <xsd:element ref="Rollen" minOccurs="0"/>
      <xsd:element ref="PublicCertificateKey" minOccurs="0"/>
      <xsd:element ref="Kommunikation" minOccurs="0"/>
      <xsd:element ref="Identifikationen" minOccurs="0"/>
      <xsd:element ref="GeburtsName" minOccurs="0"/>
      <xsd:element name="SterbeDatum" type="tpsDatumZeit" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Datum und evtl. Zeitpunkt des festgestellten Todes einer Person oder eines Subjekts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StaatsBuergerschaft" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Staatsbuergerschaft einer Person auf Basis Codeliste D28 (ISO-Codeliste, ÖNorm)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FamilienStand" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Familienstand einer Person</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="Adresse" minOccurs="0"/>
      <xsd:element name="PfadDerAufenthalte" type="xsd:string" minOccurs="0"/>
      <xsd:element ref="Beziehungen" minOccurs="0"/>
      <xsd:element name="AusuebenderBeruf" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Information zu dem ausgeübten Beruf einer Person</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:string">
              <xsd:attributeGroup ref="agrCodeList"/>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="tpcStelle">
    <xsd:all>
      <xsd:element ref="BetriebsStelleKey" minOccurs="0"/>
      <xsd:element name="GesamterName" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Name unter dem die gegenstaendliche Stelle bekannt ist (z.B. &quot;Ordination Dr. Meier&quot;)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="FamilienName" minOccurs="0"/>
      <xsd:element ref="VorName" minOccurs="0"/>
      <xsd:element ref="Titel" minOccurs="0"/>
      <xsd:element ref="eMail" minOccurs="0"/>
      <xsd:element ref="ZustellInfo" minOccurs="0"/>
      <xsd:element ref="PublicCertificateKey" minOccurs="0"/>
      <xsd:element ref="Rollen" minOccurs="0"/>
      <xsd:element ref="Adresse" minOccurs="0"/>
      <xsd:element ref="Kommunikation" minOccurs="0"/>
      <xsd:element name="StellenZusatzInformation" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Abbildung aller Felder, die im eVGA geführt werden.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="Leiter" type="tpcPerson" minOccurs="0"/>
            <xsd:element name="TechnischeStellenParameter" minOccurs="0">
              <xsd:complexType>
                <xsd:all>
                  <xsd:element name="BefundTransportProvider" type="tpsBefundTransportProvider" minOccurs="0"/>
                  <xsd:element name="TechnischeAnschlussArt" type="tpsTechnischeAnschlussArt" minOccurs="0"/>
                  <xsd:element name="AnschlussZeiten" minOccurs="0">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="AnschlussZeit" type="xsd:string" maxOccurs="unbounded">
                          <xsd:annotation>
                            <xsd:documentation>
Beschreibt wie oft diese Stelle am Netz ist. Daraus laesst sich auf voraussichtliche Antwortzeiten schliessen.
Dauernd: Dauernd am Netz, ohne Unterbrechung - Technische Empfangsbestaetigungen kommen unmittelbar, als Antwortszeiten sind nur Netzuebertragungen und Maschinenlaufzeiten moeglich
TaeglichMehrmalsBetriebsZeiten: Der Anschluss zum Netz wird waehrend der Oeffnungszeiten mehrmals hergestellt. Antwortszeiten ergeben sich aus den periodischen Anschaltzeitpunkten waehrend der Oeffnungszeiten
TaeglichEinmal: Der Anschluss wird praktisch nur einmal pro Tag hergestellt, Antwortszeiten koennen dabei sogar auf zwei Tage ausgedehnt werden, weil ja nicht sicher ist, ob waehrend der Anschaltzeit auch alle erforderlichen Antworten erzeugt werden
TaeglichManchmal: Der Anschluss wird fallweise hergestellt, eine definierte Antwortszeit kann praktisch nicht erwartet werden
                            </xsd:documentation>
                            <xsd:documentation>Dauernd: Dauernd am Netz, ohne Unterbrechung - Technische Empfangsbestaetigungen kommen unmittelbar, als Antwortszeiten sind nur Netzuebertragungen und Maschinenlaufzeiten moeglich</xsd:documentation>
                            <xsd:documentation>TaeglichMehrmalsBetriebsZeiten: Der Anschluss zum Netz wird waehrend der Oeffnungszeiten mehrmals hergestellt. Antwortszeiten ergeben sich aus den periodischen Anschaltzeitpunkten waehrend der Oeffnungszeiten</xsd:documentation>
                            <xsd:documentation>TaeglichEinmal: Der Anschluss wird praktisch nur einmal pro Tag hergestellt, Antwortszeiten koennen dabei sogar auf zwei Tage ausgedehnt werden, weil ja nicht sicher ist, ob waehrend der Anschaltzeit auch alle erforderlichen Antworten erzeugt werden</xsd:documentation>
                            <xsd:documentation>TaeglichManchmal: Der Anschluss wird fallweise hergestellt, eine definierte Antwortszeit kann praktisch nicht erwartet werden</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="AnschlussUnterbrechungsGruende" minOccurs="0">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="AnschlussUnterbrechungsGrund" maxOccurs="unbounded">
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                              <xsd:enumeration value="Betriebsferien">
                                <xsd:annotation>
                                  <xsd:documentation>Organisatorsich bedingter Betriebsstillstand, der aus den Öffnungszeiten erkennbar ist - in der Regel Betriebsferien, Übersiedlungen, ..</xsd:documentation>
                                </xsd:annotation>
                              </xsd:enumeration>
                              <xsd:enumeration value="DerzeitKeinTeilnehmer">
                                <xsd:annotation>
                                  <xsd:documentation>Derzeit ist der angesprochene Partner kein Teilnehmer am Befundversand</xsd:documentation>
                                </xsd:annotation>
                              </xsd:enumeration>
                              <xsd:enumeration value="OrganisatorischeBetriebsUnterbrechung">
                                <xsd:annotation>
                                  <xsd:documentation>Organisatorisch bedingte BetriebsUnterbrechung, die nach Änderung organisatorischer Fakten wieder weg sein wird.</xsd:documentation>
                                </xsd:annotation>
                              </xsd:enumeration>
                              <xsd:enumeration value="TechnischeBetriebsUnterbrechung">
                                <xsd:annotation>
                                  <xsd:documentation>Technisch bedingte BetriebsUnterbrechung, die nach Behebung wieder weg sein wird. Die Stillstandszeit ist nicht exakt angebbar. Der Partner verarbeitet konventionell transportierte Nachrichten - alternative Transportwege sind daher sinnvoll</xsd:documentation>
                                </xsd:annotation>
                              </xsd:enumeration>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TechnischeInterpretationsMoeglichkeiten" minOccurs="0">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="TechnischeInterpretationsMoeglichkeit" type="tpsTechnischeInterpretationsMoeglichkeit" maxOccurs="unbounded"/>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="LokaleEDVSysteme" minOccurs="0">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="LokalesEDVSystem" type="tpsLokalesEDVSystem" maxOccurs="unbounded"/>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Hinweise" minOccurs="0">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="Hinweis" type="xsd:string" maxOccurs="unbounded"/>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="ProxyEMail" type="xsd:string" minOccurs="0"/>
                  <xsd:element name="MAntwortVerhalten" minOccurs="0">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="AntwortVerhalten" type="tpsAntwortVerhalten" maxOccurs="unbounded"/>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Bemerkung" type="xsd:string" minOccurs="0"/>
                  <xsd:element name="rootVendorId" type="xsd:string" minOccurs="0"/>
                </xsd:all>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="OrganisatorischeStellenParameter" minOccurs="0">
              <xsd:complexType>
                <xsd:all>
                  <xsd:element name="OeffnungsZeit" type="tpcZeitIntervalle" minOccurs="0"/>
                  <xsd:element name="TerminVereinbarungsZeit" type="tpcZeitIntervalle" minOccurs="0"/>
                  <xsd:element name="Urlaube" minOccurs="0">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="Urlaub" maxOccurs="unbounded">
                          <xsd:complexType>
                            <xsd:all>
                              <xsd:element name="UrlaubVon" type="xsd:string" minOccurs="0"/>
                              <xsd:element name="UrlaubBis" type="xsd:string" minOccurs="0"/>
                              <xsd:element name="Vertretungen" minOccurs="0">
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element name="Vertretung" maxOccurs="unbounded">
                                      <xsd:complexType>
                                        <xsd:all>
                                          <xsd:element ref="BetriebsStelleKey" minOccurs="0"/>
                                          <xsd:element name="GesamterName" type="xsd:string" minOccurs="0"/>
                                        </xsd:all>
                                      </xsd:complexType>
                                    </xsd:element>
                                  </xsd:sequence>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:all>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Erreichbarkeiten" minOccurs="0">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="AnfahrtsWeg" maxOccurs="unbounded">
                          <xsd:complexType>
                            <xsd:all>
                              <xsd:element name="VerkehrsMittel" type="xsd:string" minOccurs="0"/>
                              <xsd:element name="Station" type="xsd:string" minOccurs="0"/>
                              <xsd:element name="GehZeit" type="xsd:string" minOccurs="0"/>
                            </xsd:all>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Beschreibung" type="xsd:string" minOccurs="0"/>
                  <xsd:element name="Versicherungen" minOccurs="0">
                    <xsd:complexType>
